    "dotenv": "^8.2.0",
    "feathers-authentication-hooks": "^1.0.2",
    "feathers-hooks-common": "^5.0.3",
    "feathers-nedb": "^5.1.0",
    "fs-jetpack": "^3.1.0",
    "helmet": "^4.1.1",
    "nedb": "^1.8.0",
    "nodemon": "^2.0.4",
    "serve-favicon": "^2.5.0",
    "ts-node": "^9.0.0",
    "winston": "^3.0.0"
  },
